/**
 * Implements hook_install().
 */
function cer_hs_install() {
  variable_set('cer_hierarchy_widget', 'CerHierarchyHSWidget');
}

/**
 * Implements hook_uninstall().
 */
function cer_hs_install() {
  cer_hs_disable();
}

/**
 * Implements hook_disable().
 */
function cer_hs_install() {
